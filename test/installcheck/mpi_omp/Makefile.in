CC  = @MPICC@
CXX = @MPICXX@
F77 = @MPIF77@
FC  = @MPIFC@

# -Ox does not work for all compilers. Use AM_CONDITIONAL here?
CFLAGS   = -O3 @OPENMP_CFLAGS@
CXXFLAGS = -O3 @OPENMP_CXXFLAGS@
F77FLAGS = -O3 @OPENMP_F77FLAGS@
FCFLAGS  = -O3 @OPENMP_FCFLAGS@

PARADIGM   = mpi_omp
CODES      = jacobi
LANGUAGES  = cc cxx fc

